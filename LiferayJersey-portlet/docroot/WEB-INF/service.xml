<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.meera.jersey.services">
	<author>meera</author>
	<namespace>meera</namespace>

	<entity name="Person" local-service="true" remote-service="false">
		<column name="personId" type="long" primary="true" />
		<column name="personName" type="String" />
		<column name="personAddress" type="String" />
	</entity>
</service-builder>
